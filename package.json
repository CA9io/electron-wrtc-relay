{
  "name": "@constl/electron-webrtc-relay",
  "version": "0.2.3",
  "description": "Use WebRTC in Electron Main Process, compatible with WebTorrent",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "testfix": "tap test/*.js -t150 -Rspec --cov",
    "test": "tap test/*.js -t150 -Rspec --cov",
    "test-tap": "tap test/*.js -Rtap",
    "test-lcov": "mkdir -p coverage && tap test/*.js -Rsilent --coverage-report=text-lcov > coverage/lcov.info",
    "publish": "tsc && npm publish --access public ."
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "webrtc",
    "electron",
    "relay",
    "wrtc",
    "webtorrent"
  ],
  "author": "TM9657 GmbH <support@ca9.io>",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4",
    "get-browser-rtc": "^1.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "electron": "^26.2.1",
    "simple-peer": "^9.11.1",
    "string-to-stream": "^3.0.1",
    "tap": "^16.3.0",
    "typescript": "^5.2.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CA9io/electron-wrtc-relay.git"
  },
  "bugs": {
    "url": "https://github.com/CA9io/electron-wrtc-relay/issues"
  },
  "homepage": "https://github.com/CA9io/electron-wrtc-relay#readme"
}
