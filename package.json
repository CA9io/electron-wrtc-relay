{
  "name": "@ca9io/electron-webrtc-relay",
  "version": "0.0.6",
  "description": "Use WebRTC in Electron Main Process, compatible with Webtorrent",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "test": "standard --fix && tap test/*.js -t150 -Rspec --cov",
    "test-tap": "tap test/*.js -Rtap",
    "test-lcov": "mkdir -p coverage && tap test/*.js -Rsilent --coverage-report=text-lcov > coverage/lcov.info",
    "pub": "npm publish --access public ."
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "webrtc",
    "electron",
    "relay",
    "wrtc",
    "webtorrent"
  ],
  "author": "TM9657 GmbH <support@ca9.io>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "get-browser-rtc": "^1.0.2",
    "hat": "^0.0.3"
  },
  "devDependencies": {
    "simple-peer": "^6.0.1",
    "electron": "^20.1.4",
    "standard": "^10.0.0",
    "string-to-stream": "^1.0.1",
    "tap": "^5.1.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CA9io/electron-wrtc-relay.git"
  },
  "bugs": {
    "url": "https://github.com/CA9io/electron-wrtc-relay/issues"
  },
  "homepage": "https://github.com/CA9io/electron-wrtc-relay#readme"
}
